        <content>
            <VBox justifyContent="Center"  alignContent="Center">
            <core:Fragment
                type="XML"
                fragmentName="ejerciciointegrador.fragments.Nav"
            />
            <core:Fragment
                type="XML"
                fragmentName="ejerciciointegrador.fragments.orders.title"
            />
            <core:Fragment
                type="XML"
                fragmentName="ejerciciointegrador.fragments.orders.orders"
            />
            </VBox>
        </content>
    </Page>
</mvc:View>
